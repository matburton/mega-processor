root=true

[*]
end_of_line=crlf
insert_final_newline=false
trim_trailing_whitespace=true
charset=utf-8
indent_style=space
indent_size=4

dotnet_naming_rule.private_instance_fields_rule.style=m_upper_camel_case_style
dotnet_naming_style.m_upper_camel_case_style.capitalization=pascal_case
dotnet_naming_style.m_upper_camel_case_style.required_prefix=m_

resharper_arrange_object_creation_when_type_not_evident_highlighting=none
resharper_variable_hides_outer_variable_highlighting=none
resharper_explicit_caller_info_argument_highlighting=none
